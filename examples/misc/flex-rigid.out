Abella < Define fdlist : (olist -> olist) -> olist -> prop by 
fdlist (l\l) nil;
fdlist (l\X :: F l) (X :: L) := fdlist F L.

Abella < Define append : olist -> olist -> olist -> prop by 
append nil C C;
append (X :: A) B (X :: C) := append A B C.

Abella < Theorem fdlist_det1 : 
forall F L1 L2, fdlist F L1 -> fdlist F L2 -> L1 = L2.


  ============================
   forall F L1 L2, fdlist F L1 -> fdlist F L2 -> L1 = L2

fdlist_det1 < induction on 1.


  IH : forall F L1 L2, fdlist F L1 * -> fdlist F L2 -> L1 = L2
  ============================
   forall F L1 L2, fdlist F L1 @ -> fdlist F L2 -> L1 = L2

fdlist_det1 < intros.

  Variables: F, L1, L2
  IH : forall F L1 L2, fdlist F L1 * -> fdlist F L2 -> L1 = L2
  H1 : fdlist F L1 @
  H2 : fdlist F L2
  ============================
   L1 = L2

fdlist_det1 < case H1.
Subgoal 1:

  Variables: F, L1, L2
  IH : forall F L1 L2, fdlist F L1 * -> fdlist F L2 -> L1 = L2
  H2 : fdlist (l\l) L2
  ============================
   nil = L2

Subgoal 2 is:
 X :: L = L2

fdlist_det1 < case H2.
Subgoal 1:

  Variables: F, L1, L2
  IH : forall F L1 L2, fdlist F L1 * -> fdlist F L2 -> L1 = L2
