Abella < Kind name, action, proc type.

Abella < Type tau action.

Abella < Type up, dn name -> action.

Abella < Type null proc.

Abella < Type plus, par proc -> proc -> proc.

Abella < Type act action -> proc -> proc.

Abella < Type repl proc -> proc.

Abella < Define one : proc -> action -> proc -> prop by 
one (act A P) A P;
one (plus P1 P2) A Q := one P1 A Q;
one (plus P1 P2) A Q := one P2 A Q;
one (par P Q) A (par P1 Q) := one P A P1;
one (par P Q) A (par P Q1) := one Q A Q1;
one (repl P) A (par (repl P) Q) := one P A Q;
one (par P Q) tau (par P1 Q1) := exists X, one P (up X) P1 /\ one Q (dn X) Q1;
one (repl P) tau (par (repl P) (par Q R)) := exists X, one P (up X) Q /\ one P (dn X) R.

Abella < Kind ctx type.

Abella < Type emptyCtx ctx.

Abella < Type noHole proc -> ctx.

Abella < Type act_ctx action -> ctx -> ctx.

Abella < Type par_ctx_l, plus_ctx_l ctx -> proc -> ctx.

Abella < Type par_ctx_r, plus_ctx_r proc -> ctx -> ctx.

Abella < Define in_ctx : ctx -> proc -> proc -> prop by 
in_ctx emptyCtx P P;
in_ctx (noHole P) Q P;
in_ctx (par_ctx_l C Q) P (par R Q) := in_ctx C P R;
in_ctx (par_ctx_r P C) Q (par P R) := in_ctx C Q R;
in_ctx (plus_ctx_l C Q) P (plus R Q) := in_ctx C P R;
in_ctx (plus_ctx_r P C) Q (plus P R) := in_ctx C Q R;
in_ctx (act_ctx A C) P (act A R) := in_ctx C P R.

Abella < Define equiv_rel : (proc -> proc -> prop) -> prop by 
equiv_rel Rel := (forall P, Rel P P) /\ (forall P Q, Rel P Q -> Rel Q P) /\ (forall P Q R,
  Rel P Q -> Rel Q R -> Rel P R).

Abella < Define subst_rel : (proc -> proc -> prop) -> prop by 
subst_rel Rel := forall P Q C P' Q', Rel P Q -> in_ctx C P P' -> in_ctx C Q Q' -> Rel P' Q'.

Abella < Define cong_rel : (proc -> proc -> prop) -> prop by 
cong_rel Rel := equiv_rel Rel /\ subst_rel Rel.

Abella < CoDefine bisim_up_to : (proc -> proc -> proc -> proc -> prop) -> proc -> proc -> prop by 
bisim_up_to Upto P Q := (forall A P1, one P A P1 -> (exists Q1, one Q A Q1 /\ (exists P2 Q2,
  Upto P1 P2 Q1 Q2 /\ bisim_up_to Upto P2 Q2))) /\ (forall A Q1,
  one Q A Q1 -> (exists P1, one P A P1 /\ (exists P2 Q2, Upto P1 P2 Q1 Q2 /\
  bisim_up_to Upto P2 Q2))).

Abella < Define refl_t : proc -> proc -> proc -> proc -> prop by 
refl_t P P Q Q.

Abella < CoDefine bisimr : proc -> proc -> prop by 
bisimr P Q := bisim_up_to refl_t P Q.

